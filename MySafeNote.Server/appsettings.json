{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/mysafenote.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
    "AllowedHosts": "*",
    "ConnectionStrings": {
      "Default": "DataSource=MySafeNoteDb.sqlite"
    }
}
